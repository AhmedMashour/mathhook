{
  "name": "mathhook-node-examples",
  "version": "1.0.0",
  "description": "TypeScript examples for MathHook Node.js bindings",
  "private": true,
  "scripts": {
    "basic": "tsx basic-usage.ts",
    "advanced": "tsx advanced-usage.ts",
    "install-deps": "npm install",
