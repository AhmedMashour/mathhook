    "build-binding": "cd .. && napi build",
    "clean": "cd .. && cargo clean"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "mathematics",
    "symbolic",
    "algebra",
    "typescript",
    "examples"
  ]
}
