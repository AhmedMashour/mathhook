{
  "name": "mathhook-node",
  "version": "0.1.2",
  "description": "Node.js bindings for MathHook mathematical engine",
  "author": "Ahmed Mashhour <mpghknown@gmail.com>",
  "main": "index.js",
  "module": "index.mjs",
  "types": "index.d.ts",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "napi": {
    "binaryName": "mathhook-node",
    "targets": [
      "x86_64-unknown-linux-gnu",
      "x86_64-unknown-linux-musl",
      "aarch64-unknown-linux-gnu",
      "aarch64-unknown-linux-musl",
      "aarch64-apple-darwin",
      "universal-apple-darwin",
      "armv7-unknown-linux-gnueabihf",
      "i686-pc-windows-msvc",
      "x86_64-pc-windows-msvc",
      "aarch64-pc-windows-msvc"
    ]
  },
  "license": "MIT OR Apache-2.0",
  "devDependencies": {
    "@ava/typescript": "^6.0.0",
    "@napi-rs/cli": "^3.5.0",
    "@types/node": "^25.0.3",
    "ava": "^6.4.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "ava": {
    "timeout": "3m",
    "extensions": [
      "ts"
    ],
    "files": [
      "tests/**/*.ts"
    ],
    "require": [
      "tsx"
    ]
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "ava",
    "bench": "tsx bench_bindings.ts",
    "typecheck": "tsc --noEmit",
    "universal": "napi universal",
    "version": "napi version"
  },
  "keywords": [
    "mathematics",
    "symbolic",
    "algebra",
    "calculus",
    "equation",
    "solver",
    "napi-rs"
  ],
  "files": [
    "index.d.ts",
    "index.js",
    "*.node"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AhmedMashour/mathhook.git"
  },
  "homepage": "https://github.com/AhmedMashour/mathhook",
  "bugs": {
    "url": "https://github.com/AhmedMashour/mathhook/issues"
  },
  "optionalDependencies": {
    "mathhook-node-linux-x64-gnu": "0.1.2",
    "mathhook-node-linux-x64-musl": "0.1.2",
    "mathhook-node-linux-arm64-gnu": "0.1.2",
    "mathhook-node-linux-arm64-musl": "0.1.2",
    "mathhook-node-darwin-arm64": "0.1.2",
    "mathhook-node-darwin-universal": "0.1.2",
    "mathhook-node-linux-arm-gnueabihf": "0.1.2",
    "mathhook-node-win32-ia32-msvc": "0.1.2",
    "mathhook-node-win32-x64-msvc": "0.1.2",
    "mathhook-node-win32-arm64-msvc": "0.1.2"
  }
}